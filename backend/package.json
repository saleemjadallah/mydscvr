{
  "name": "headshotsaas-backend",
  "version": "1.0.0",
  "description": "HeadShotHub Backend API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp -r drizzle dist/ && node scripts/fix-schema-imports.js",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push --force",
    "db:studio": "drizzle-kit studio",
    "db:fix": "tsx scripts/fix-production-schema.ts",
    "db:rebuild": "tsx scripts/rebuild-database.ts",
    "test:gemini": "tsx scripts/test-gemini-pro.ts"
  },
  "dependencies": {
