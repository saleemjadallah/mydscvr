    "@google/generative-ai": "^0.24.1",
    "@thednp/dommatrix": "^2.0.12",
    "@types/pdfkit": "^0.17.3",
    "archiver": "^6.0.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.1.9",
    "canvas": "^3.2.0",
    "connect-pg-simple": "^9.0.1",
    "cookie-parser": "^1.4.7",
